{
  "name": "api-front",
  "version": "0.1.0",
  "license": "private",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "e2e": "ng e2e",
    "prettier": "npm run prettier-ts && npm run prettier-scss && npm run prettier-json && npm run prettier-markdown",
    "prettier-ts": "prettier --print-width 120 --tab-width 2 --single-quote --write \"src/app/**/*.ts\"",
    "prettier-scss": "prettier --tab-width 2 --single-quote --write \"src/app/**/*.scss\"",
    "prettier-json": "prettier --tab-width 2 --write \"src/app/**/*.json\" \"src/asset/localdb/**/*.json\" ",
    "prettier-markdown": "prettier --write \"README.md\" \"src/app/*.md\"",
    "lint": "ng lint",
    "pre-commit-msg": "echo 'Linting start ...' && exit 0",
    "next-commit-msg": "echo 'Lint success' && exit 0",
    "tslint": "tslint -c tslint.json 'src/app/**/*.ts' -e 'src/app/**/*/.spec.ts'"
  },
  "pre-commit": [
    "pre-commit-msg",
    "prettier",
    "lint",
    "next-commit-msg"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.1.1",
    "@angular/cdk": "^5.0.1",
    "@angular/common": "^5.1.1",
    "@angular/compiler": "^5.1.1",
    "@angular/core": "^5.1.1",
    "@angular/forms": "^5.1.1",
    "@angular/http": "^5.1.1",
    "@angular/material": "^5.0.1",
    "@angular/platform-browser": "^5.1.1",
    "@angular/platform-browser-dynamic": "^5.1.1",
    "@angular/router": "^5.1.1",
    "animate.css": "^3.5.2",
    "bootstrap": "^4.0.0-beta.2",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.3",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "ng2-ckeditor": "^1.1.13",
    "ngx-cropper": "^1.2.1",
    "ngx-toastr": "^8.0.0",
    "popper.js": "^1.13.0",
    "rxjs": "^5.5.5",
    "sweetalert2": "^7.1.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.6.0",
    "@angular/compiler-cli": "^5.1.1",
    "@angular/language-service": "^5.1.1",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.2",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "pre-commit": "^1.2.2",
    "prettier": "1.9.1",
    "protractor": "~5.1.2",
    "sass-lint": "^1.12.1",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  },
  "sasslintConfig": ".sass-lint.yml"
}
